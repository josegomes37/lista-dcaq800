#include <stdio.h>
int main(){
    int ano;

    printf("o ano eh: ");
    scanf("%d", &ano);

    if((an%400)==0){
        printf("o ano %d é bissexto\n", ano);
    }else{
     if((ano%4)==0 && (ano%100)!=0){
            printf("o ano %d é bissexto\n", ano);
    }else{
            printf("o ano %d não é bissexto\n", ano);
}
}
}
